{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "staticWebAppName": {
      "value": "my-swa-app"
    },
    "location": {
      "value": "East Asia"
    },
    "repositoryUrl": {
      "value": "https://github.com/your-username/your-repo-name"
    },
    "branch": {
      "value": "main"
    },
    "repositoryToken": {
      "value": "YOUR_GITHUB_PERSONAL_ACCESS_TOKEN"
    },
    "appLocation": {
      "value": "frontend"
    },
    "apiLocation": {
      "value": "api"
    },
    "outputLocation": {
      "value": "out"
    }
  }
}
