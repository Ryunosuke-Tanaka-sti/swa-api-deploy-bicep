{
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "clientIdSettingName": "GITHUB_CLIENT_ID",
          "clientSecretSettingName": "GITHUB_PROVIDER_AUTHENTICATION_SECRET"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/protected/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github",
      "statusCode": 302
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
